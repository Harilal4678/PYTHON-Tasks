
def find_maximum(numbers):
    if not numbers:  
        return None
    
    maximum = numbers[0]  
    for num in numbers:
        if num > maximum:
            maximum = num  
    return maximum

# Example usage
numbers_list = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3]
print("The maximum number is:", find_maximum(numbers_list))




